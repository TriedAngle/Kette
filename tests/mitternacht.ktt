( ( |
  rootsOfA: a B: b C: c = (
    four = 4.
    disc := (b * b) - (four * a * c).
    sqrtDisc := disc sqrt.
    denom := 2 * a.
    root1 := ((0 - b) + sqrtDisc) / denom.
    root2 := ((0 - b) - sqrtDisc) / denom.
    @( root1. root2 )
  ) .
  doNothing = ( ) 
| ) rootsOfA: 1 B: -3 C: 2 ) each: [ | :element | element print ]
