// expect:
// -1 + 1i
// -1 - 1i

Module open: 'Test::Mitternacht.
Module use: 'Math.

Quadratic = {
    printRootsOfA: a B: b C: c = {
        disc = (b * b) - (4 * a * c).
        sqrtDisc = disc sqrt.
        denom = 2 * a.
        negB = 0 - b.
        ((negB + sqrtDisc) / denom) toString println.
        ((negB - sqrtDisc) / denom) toString println.
    }.
}.

Quadratic printRootsOfA: 1 B: 2 C: 2.
